{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0 && prettier --check src/**/*.jsx",
    "lintfix": "npx eslint --fix src/**/*.jsx",
    "format": "prettier --write --list-different src/**/*.jsx",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.5",
    "@floating-ui/react-dom": "^2.0.5",
    "classnames": "^2.5.1",
    "initials": "^3.1.2",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "sass": "^1.69.3",
    "vite": "^4.4.11",
    "vite-plugin-babel-macros": "^1.0.6"
  }
}
